# Real-time AI Voice Translator - Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI API (for GPT translation)
OPENAI_API_KEY=your_openai_api_key_here

# Google API (for Gemini translation)
GOOGLE_API_KEY=your_google_api_key_here

# Groq API (optional, for ultra-fast inference)
GROQ_API_KEY=your_groq_api_key_here

# Model paths (optional, defaults will be used if not set)
# TTS_MODEL_PATH=./models/f5_tts
# ASR_MODEL_PATH=./models/whisper

